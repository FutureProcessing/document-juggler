{
  "Version": "2008-10-17",
  "Id": "Policy1421843909584",
  "Statement": [
    {
      "Sid": "AllowPublicRead",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetBucketNotification",
        "s3:GetBucketAcl",
        "s3:ListBucketVersions",
        "s3:GetObjectAcl",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:GetBucketLogging",
        "s3:GetObject",
        "s3:GetLifecycleConfiguration",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning",
        "s3:GetObjectVersionAcl"
      ],
      "Resource": [
        "arn:aws:s3:::fp-maven-repository",
        "arn:aws:s3:::fp-maven-repository/*"
      ]
    }
  ]
}